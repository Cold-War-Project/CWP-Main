### default fade in/out
template default_fade_in_out
{
	#alpha = 0 #BUGGY? this makes widgets that are already visible on game start not show.
	
	state = {
		name = _show
		using = default_show_properties
	}
	state = {
		name = _hide
		#using = default_hide_properties #BUGGY? this makes widgets that should be hidden fade away over time when you open interfaces for the first time.
		duration = 0 
		alpha = 0
	}
}

### default window content fade (used mostly for tabs)
template default_content_fade
{
	state = {
		name = _show
		using = default_show_properties
	}
	state = {
		name = _hide
		#keep duration at 0 anything else screw things up.
		duration = 0 
		alpha = 0
	}
}

### Window content refresh animation
template Animation_Refresh_FadeOut
{
    alpha = 0.5
    next = "fadein"
    using = Animation_Curve_Default
}

template Animation_Refresh_FadeIn
{
    name = "fadein"
    alpha = 1
    duration = 0.4
    bezier = { 0.5 0 1 0.5 }
}

### default show/hide properties
template default_show_properties
{
	duration = 0.2
	alpha = 1
	using = Animation_Curve_Default
}
template default_show_properties_fast
{
	duration = 0.15
	alpha = 1
	using = Animation_Curve_Default
}
template default_show_properties_slow
{
	duration = 0.5
	alpha = 1
	using = Animation_Curve_Default
}

template default_hide_properties
{
	using = window_hide_duration
	alpha = 0
	using = Animation_Curve_Default
}
template default_hide_properties_fast
{
	duration = 0.15
	alpha = 0
	using = Animation_Curve_Default
}

### default fade in animation
template default_fade_in
{
	state = {
		block "trigger" {
			name = start
			trigger_on_create = yes
		}
		next = 2
		duration = 0
		alpha = 0
	}
	state = {
		name = 2
		alpha = 1
		block "duration" {
			duration = 0.5
		}
		using = Animation_Curve_Default
	}
}

### for buttons with frames in the texture
template default_button_animation
{
	# made duration 0 for now because it caused some annoying flimmer otherwise.
	state = {
		name = _show
		#using = button_animation_duration
		duration = 0
		frame = 1
	}		
	state = {
		name = _mouse_enter
		#using = button_animation_duration
		duration = 0
		frame = 2
	}		
	state = {
		name = _mouse_leave
		#using = button_animation_duration
		duration = 0
		frame = 1
	}		
	state = {
		name = _mouse_press
		#using = button_animation_duration
		duration = 0
		frame = 3
	}		
	state = {
		name = _mouse_release
		#using = button_animation_duration
		duration = 0
		frame = 2
	}
}

### for buttons that uses modify_textures as mouseover etc.
template default_mouse_states_animation 
{	

	### mouse enter
	state = {
		name = _mouse_enter
		using = Animation_Curve_Default
		using = button_animation_duration
		modify_texture = {
			name = mouse_press
			alpha = 0
		}
		modify_texture = {
			name = mouse_enter
			alpha = 1
		}
	}
	
	### mouse leave
	state = {
		name = _mouse_leave
		using = button_animation_duration_slow
		modify_texture = {
			name = mouse_press
			alpha = 0
		}
		modify_texture = {
			name = mouse_enter
			alpha = 0
		}
	}
	
	### mouse press
	state = {
		name = _mouse_press
		using = button_animation_duration
		modify_texture = {
			name = mouse_press
			alpha = 1
		}
		modify_texture = {
			name = mouse_enter
			alpha = 0
		}
	}
	
	### mouse release
	state = {
		name = _mouse_release
		using = button_animation_duration
		modify_texture = {
			name = mouse_press
			alpha = 0
		}
		modify_texture = {
			name = mouse_enter
			alpha = 1
		}
	}
}

### SPECIAL EFFECTS
template shimmer_fade_in {
	modify_texture = {
		name = "glow"
		texture = "gfx/interface/animation/shimmer.dds"
		blend_mode = colordodge
		translate_uv = { 0 0 }
	}

	state = {
		block "trigger" {
			trigger_on_create = yes
			name = start
		}
		next = shimmer
		duration = 0
		alpha = 0

		modify_texture = {
			name = "glow"
			translate_uv = { -0.5 -0.5 }
		}
	}

	state = {
		name = shimmer
		duration = 1
		alpha = 1
		
		using = Animation_Curve_Default

		modify_texture = {
			name = "glow"
			translate_uv = { 1 1 }
		}
	}
}

template hud_shiny_effect {
	modify_texture = {
		name = "glow"
		texture = "gfx/interface/animation/shimmer.dds"
		blend_mode = colordodge
		translate_uv = { -1 -1 }
	}
	
	state = {
		name = game_start_animation_effect
		next = game_start_animation_effect2
		
		modify_texture = {
			name = "glow"
			translate_uv = { -1 -1 }
		}
	}
	state = {
		name = game_start_animation_effect2
		duration = 3
		delay = 1.8 #delay for clouds to vanish before this is played
		
		modify_texture = {
			name = "glow"
			translate_uv = { 1 1 }
		}
	}
}

template shimmer {
	modify_texture = {
		name = "glow"
		texture = "gfx/interface/animation/shimmer.dds"
		blend_mode = colordodge
		translate_uv = { 0 0 }
		block "shimmer_visibility" {}
	}

	state = {
		trigger_on_create = yes
		name = show
		next = shimmer
		duration = 1

		modify_texture = {
			name = "glow"
			translate_uv = { -1 -1 }
		}
	}

	state = {
		name = shimmer
		next = pause
		duration = 2
		start_sound = {
			soundeffect = "event:/SFX/UI/Global/shimmer"
		}
		
		bezier = { 0 0.9 1 0.4 }

		modify_texture = {
			name = "glow"
			translate_uv = { 1 1 }
		}

		block "shimmer_animation_properties" {}
	}

	state = {
		name = pause
		next = shimmer
		duration = 0
		delay = 6

		modify_texture = {
			name = "glow"
			translate_uv = { -1 -1 }
		}
	}	
}

