types animation_assets {
	
	### CHECKMARK ANIMATION
	type checkmark_animation = widget {
		size = { 50 50 }
		
		icon = {
			parentanchor = center
			size = { 100% 100% }
			
			texture = "gfx/interface/icons/generic_icons/checkmark.dds"
			block "visible" {}
			
			state = {
				block "trigger" {
					name = _show
				}
				next = 2
				
				scale = 0.1
				alpha = 0
			}
			state = {
				name = 2
				next = 3
				
				scale = 1.4
				alpha = 1
				
				#delay = 0.1
				duration = 0.2
				using = Animation_Curve_Default
			}
			state = {
				name = 3
				
				scale = 1
				
				duration = 0.1
				using = Animation_Curve_Default
