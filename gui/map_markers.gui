							tooltip = "HQ_PANEL_FLOTILLAS_IN_RESERVE"
							# Set a height, otherwise autoresize would calculate a rational height (21.66666) which in some way leads to map marker flickering:
							# at one frame the map marker is small and cursor intersects it, at the next frame the map marker expands but the same cursor position doesn't intersect it
							minimumsize = { 120 22 } 
							alpha = 0.6
							autoresize = yes
							text = "HQ_IN_RESERVE"
							align = hcenter|nobaseline
						}
					}
				}
