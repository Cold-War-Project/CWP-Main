							tooltip = "HQ_PANEL_BATTALIONS_IN_RESERVE"
							alpha = 0.6
							autoresize = yes
							text = "HQ_IN_RESERVE"
							align = hcenter|nobaseline
							# Set a height, otherwise autoresize would calculate a rational height (21.66666) which in some way leads to map marker flickering:
